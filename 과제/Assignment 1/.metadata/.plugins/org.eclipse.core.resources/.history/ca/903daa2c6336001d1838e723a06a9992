public class Main {
	//main method
	public static void main(String[] args) {
		//create studentObject
		Student studentObject = new Student();
		//call studentInfo method from Student class
		studentObject.studentInfo();
		
		//print student information
		System.out.println("========================");
		System.out.println("Student name: " + studentObject.studentName);
		System.out.println("Student's Age: " + studentObject.studentAge);
		System.out.println("Student's CGPA: " + studentObject.cumulativeGPA);
		System.out.println("Student's department: " + studentObject.studentDepartment);
	}

}
